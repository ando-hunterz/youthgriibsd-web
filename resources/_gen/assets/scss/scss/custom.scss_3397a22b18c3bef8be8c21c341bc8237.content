input{appearance:textfield;border-radius:10px 10px 10px 10px;border-width:1px;text-align:center;padding:10px;width:100%}input::-webkit-input-placeholder{opacity:.5}input[type=radio]{width:10%}button{background:repeat #0f0f0f;border-color:#fff #327cb5 #327cb5 #fff;border-radius:10px 10px 10px 10px;border-style:solid;border-width:1px;box-shadow:1px 1px 3px #333;color:#fff;cursor:pointer;font-weight:700;padding:5px;text-align:center;width:100%}.iframe-rwd{position:relative;padding-bottom:65.25%;padding-top:30px;height:0;overflow:hidden}.iframe-rwd iframe{position:absolute;top:0;left:0;width:100%;height:100%}